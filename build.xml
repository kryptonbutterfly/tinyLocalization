<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE xml>
<project default="create_jar" name="Package Localization into a jarfile">
	<target name="create_jar">
		<jar destfile="build/Localization.jar">
			<manifest>
				<attribute name="Created-By" value="kryptonbutterfly"/>
				<attribute name="Class-Path" value="."/>
			</manifest>
			<fileSet dir="bin"/>
		</jar>
	</target>
</project>